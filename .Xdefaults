!TABS
URxvt.perl-ext-common: default,tabbed,clipboard,url-select,keyboard-select,fullscreen,resize-font
URxvt.tabbed.tabbar-fg: 2
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 3
URxvt.tabbed.tab-bg: 0

URxvt.url-select.launcher:  brave
URxvt.url-select.underline: true
URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search

URxvt.keysym.F11: perl:fullscreen:switch

URxvt.iso14755: false
URxvt.iso14755_52: false


!FONTS
URxvt*font: xft:Fira Code:size=14:antialias=true
URxvt*boldFont: xft:Fira Code:size=14:antialias=true
! URxvt.letterSpace: -2
